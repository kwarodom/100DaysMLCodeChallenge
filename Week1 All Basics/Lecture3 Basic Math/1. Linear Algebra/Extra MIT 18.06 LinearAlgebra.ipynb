    "\n",
    "$\\left[\\begin{matrix}\n",
    "1&-1\\\\\n",
    "1&-1\n",
    "\\end{matrix}\\right]\n",
    "\\left[\\begin{matrix}\n",
    "1&1\\\\\n",
    "1&1\n",
    "\\end{matrix}\\right]=\n",
    "\\left[\\begin{matrix}\n",
    "0&0\\\\\n",
    "0&0\n",
    "\\end{matrix}\\right]\n",
